  <body>
    <main class="container">
      <header class="row">
        <h1 class="col">Pikachu Game</h1>
      </header>
      <section class="row" id="app">
        <div class="col-6 offset-3 mt-3">
          <form action="board">
            <div class="form-group">
              <label for="">Username</label>
              <input
                type="text"
                class="form-control"
                id="username"
                placeholder="Nhập tên vào"
              />
              <small id="userNameHelpId" class="form-text text-muted invisible"
                >NO NO</small
              >
            </div>
            <div class="form-group">
              <input
                type="button"
                id="Play"
                class="btn btn-primary"
                value="Play game"
              />
            </div>
          </form>
        </div>
      </section>
    </main>
    <script src="assets/user.bundle.js"></script>
    <script src="assets/vendors~user.chunk.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.slim.min.js"></script>
